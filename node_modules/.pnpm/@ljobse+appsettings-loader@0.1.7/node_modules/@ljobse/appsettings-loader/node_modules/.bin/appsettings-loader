#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sergejdeev/Documents/projects/panenco-node-course/node_modules/.pnpm/@ljobse+appsettings-loader@0.1.7/node_modules/@ljobse/appsettings-loader/lib/node_modules:/Users/sergejdeev/Documents/projects/panenco-node-course/node_modules/.pnpm/@ljobse+appsettings-loader@0.1.7/node_modules/@ljobse/appsettings-loader/node_modules:/Users/sergejdeev/Documents/projects/panenco-node-course/node_modules/.pnpm/@ljobse+appsettings-loader@0.1.7/node_modules/@ljobse/node_modules:/Users/sergejdeev/Documents/projects/panenco-node-course/node_modules/.pnpm/@ljobse+appsettings-loader@0.1.7/node_modules:/Users/sergejdeev/Documents/projects/panenco-node-course/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/sergejdeev/Documents/projects/panenco-node-course/node_modules/.pnpm/@ljobse+appsettings-loader@0.1.7/node_modules/@ljobse/appsettings-loader/lib/node_modules:/Users/sergejdeev/Documents/projects/panenco-node-course/node_modules/.pnpm/@ljobse+appsettings-loader@0.1.7/node_modules/@ljobse/appsettings-loader/node_modules:/Users/sergejdeev/Documents/projects/panenco-node-course/node_modules/.pnpm/@ljobse+appsettings-loader@0.1.7/node_modules/@ljobse/node_modules:/Users/sergejdeev/Documents/projects/panenco-node-course/node_modules/.pnpm/@ljobse+appsettings-loader@0.1.7/node_modules:/Users/sergejdeev/Documents/projects/panenco-node-course/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/cli.js" "$@"
else
  exec node  "$basedir/../../lib/cli.js" "$@"
fi
